{
  "manifest_version": 3,
  "name": "Phishing Detector",
  "version": "1.0",
  "description": "Phishing extension for detecting phishing attempts",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "webNavigation",
    "notifications",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}